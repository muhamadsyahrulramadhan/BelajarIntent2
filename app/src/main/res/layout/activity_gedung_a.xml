<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/activity_gedung_a"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ActivityGedung_A">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#3cddf5"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="16dp">
        <ImageView
            android:id="@+id/imagejudul"
            android:layout_width="90dp"
            android:layout_height="65dp"
            android:src="@drawable/chair"
            android:layout_marginLeft="15dp"
            android:paddingTop="2dp"
            android:paddingLeft="30dp"/>
        <TextView
            android:id="@+id/judul1"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:text="Lihat Ruangan"
            android:textSize="25sp"
            android:textColor="#000"
            android:layout_marginLeft="130dp"
            android:paddingTop="15dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="400dp">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:text="Gedung A"
            android:textSize="25dp"
            android:textStyle="bold"
            android:textColor="#000"
            android:textAlignment="center"
            android:layout_marginTop="20dp"/>
        <Button
            android:id="@+id/btn1"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A1.1"
            android:textColor="#ffffff"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/btn2"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A1.2"
            android:textColor="#ffffff"
            android:layout_toRightOf="@+id/btn1"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="20dp"/>
        <Button
            android:id="@+id/btn3"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A1.3"
            android:textColor="#ffffff"
            android:layout_toRightOf="@+id/btn2"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="60dp"/>
        <Button
            android:id="@+id/btn4"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A1.4"
            android:textColor="#ffffff"
            android:layout_toRightOf="@+id/btn3"
            android:layout_marginTop="100dp"
            android:layout_marginLeft="20dp"/>

        <Button
            android:id="@+id/btn5"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:layout_below="@+id/btn1"
            android:text="A1.5"
            android:textColor="#ffffff"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/btn6"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A2.1"
            android:textColor="#ffffff"
            android:layout_below="@+id/btn2"
            android:layout_toRightOf="@+id/btn5"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="20dp"/>
        <Button
            android:id="@+id/btn7"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A2.2"
            android:textColor="#ffffff"
            android:layout_below="@+id/btn3"
            android:layout_toRightOf="@+id/btn6"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="60dp"/>
        <Button
            android:id="@+id/btn8"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A2.3"
            android:textColor="#ffffff"
            android:layout_below="@+id/btn4"
            android:layout_toRightOf="@+id/btn7"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="20dp"/>

        <Button
            android:id="@+id/btn9"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:layout_below="@+id/btn5"
            android:text="A2.4"
            android:textColor="#ffffff"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="30dp"/>
        <Button
            android:id="@+id/btn10"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A2.5"
            android:textColor="#ffffff"
            android:layout_below="@+id/btn6"
            android:layout_toRightOf="@+id/btn9"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="20dp"/>
        <Button
            android:id="@+id/btn11"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A2.6"
            android:textColor="#ffffff"
            android:layout_below="@+id/btn7"
            android:layout_toRightOf="@+id/btn10"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="60dp"
            android:onClick="duaenam"/>
        <Button
            android:id="@+id/btn12"
            android:layout_width="50dp"
            android:layout_height="20dp"
            android:textSize="15dp"
            android:background="@drawable/curva"
            android:text="A2.7"
            android:textColor="#ffffff"
            android:layout_below="@+id/btn8"
            android:layout_toRightOf="@+id/btn11"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="20dp"/>
    </RelativeLayout>

</LinearLayout>